{
	"name": "squire-core",
	"type": "module",
	"scripts": {
		"lint:fix": "bunx biome check --write .",
		"test": "bun run --filter='*' test",
		"test:cover": "vitest --coverage",
		"db:clean": "rm -rf .db && mkdir .db",
		"clean:all": "rimraf .db rimraf node_modules apps/**/node_modules",
		"dev:worker": "bun apps/pre-load/index.ts",
		"dev:app": "bun run --filter client dev",
		"dev:build": "bun run --filter client build"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/bun": "latest",
		"@types/node-cron": "^3.0.11",
		"@vitest/coverage-v8": "^3.0.4",
		"jsdom": "^25.0.1",
		"sveltekit-sse": "^0.13.9"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"workspaces": [
		"packages/*",
		"apps/*"
	],
	"dependencies": {
		"duckdb-async": "^1.1.3",
		"node-cron": "^3.0.3",
		"pino": "^9.5.0",
		"pino-caller": "^3.4.0",
		"vitest": "^3.0.4",
		"zod": "^3.23.8"
	},
	"packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}